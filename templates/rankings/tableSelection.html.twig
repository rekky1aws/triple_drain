{% extends 'base.html.twig' %}

{% block title %} {{controller_name}} {% endblock %}

{% block body %}
{# List all tables with links to table rankings #}
<ul>
  {% for table in tables %}
  <li><a href="{{ path('app_rankings_table', {id: table.id}) }}">{{ table.name }}</a></li>
  {% endfor %}
</ul>
{% endblock %}
